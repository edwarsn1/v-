Private Sub cmdOK_Click()
  Me.Hide
End Sub

Private Sub Form_Load()
  Const PROCEDURE_NAME As String = "Form_Load"
  
  PositionForm Me

  grdConstants.Row = 0
  grdConstants.Col = 0
  grdConstants.Array = XConstants
  grdConstants.Refresh
  grdFactors.Row = 0
  grdFactors.Col = 0
  grdFactors.Array = XFactors
  grdFactors.Refresh
  If gs_myTable = "Constants" Then
    tabConstants.Tab = 0
  Else
    tabConstants.Tab = 1
  End If
  Exit Sub

Error_Handler:

   gud_PrgErr.Number = Err.Number
   gud_PrgErr.Severity = 5
   gud_PrgErr.Description = Err.Description
   gud_PrgErr.Module = MODULE_NAME
   gud_PrgErr.Procedure = PROCEDURE_NAME
   Call Display_UI_Error

End Sub

Private Sub grdConstants_FormatText(ByVal ColIndex As Integer, Value As Variant, Bookmark As Variant)
  If (ColIndex = 1) Then
    If (Value > 1) Then
      Value = Format(Value, "0.00000000E+00")
    Else
      Value = Format(Value, "0.00000000E-00")
    End If
  End If
End Sub

Private Sub grdConstants_MouseUp(Button As Integer, Shift As Integer, x As Single, y As Single)
' X and Y are window coordinates
Dim CN As Integer, RN As Integer, BM As Variant
  
  If Button = vbRightButton Then
    RN = grdConstants.RowContaining(y)
    If (RN >= 0) Then
      BM = grdConstants.RowBookmark(RN)
      CN = grdConstants.ColContaining(x)
      If (CN >= 0) Then
        Clipboard.SetText grdConstants.Columns(CN).CellText(BM)
      End If
    End If
  End If

End Sub

Private Sub grdFactors_FormatText(ByVal ColIndex As Integer, Value As Variant, Bookmark As Variant)
  If (ColIndex = 1) Then
    If (Value > 1) Then
      Value = Format(Value, "0.00000000E+00")
    Else
      Value = Format(Value, "0.00000000E-00")
    End If
  End If
End Sub

Private Sub grdFactors_MouseUp(Button As Integer, Shift As Integer, x As Single, y As Single)
' X and Y are window coordinates
Dim CN As Integer, RN As Integer, BM As Variant
  
  If Button = vbRightButton Then
    RN = grdFactors.RowContaining(y)
    If (RN >= 0) Then
      BM = grdFactors.RowBookmark(RN)
      CN = grdFactors.ColContaining(x)
      If (CN >= 0) Then
        Clipboard.SetText grdFactors.Columns(CN).CellText(BM)
      End If
    End If
  End If
  
End Sub
