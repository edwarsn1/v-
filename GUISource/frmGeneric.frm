Public Sub InitControls(Cap As String, times, Values, _
                Showselect As Boolean, Subtitle As String, Title As String)
' Called from SetHeadings()

Dim i, j
Dim AControl As Control
Dim Mode As Integer '3.10 for non-Iodine Aerosols JCK 11/2005
Dim Diff As Single

   For Each AControl In Controls
     If TypeOf AControl Is TextBox Then AControl.Text = ""
    If TypeOf AControl Is TextBox Or TypeOf AControl Is ComboBox Then
      AControl.BackColor = vbWhite
      AControl.ForeColor = vbBlack
    End If
  Next
  
    Caption = Cap
    lblData(0) = Title
    lblData(1) = Subtitle
    If HeaderType = "dfTPSupo" Then
      If frmPathway.optAerosol(0) Then
        Mode = 0
        lblHeading(8) = "Elemental"
        lblHeading(9) = "Organic"
     Else
        Mode = 1
        lblHeading(8) = "Iodine"
        lblHeading(9) = ""
        With Shape1
            Diff = .Width - 8295
            .Width = 8295
        End With
        cmdOK.Left = cmdOK.Left - Diff
        cmdCancel.Left = cmdOK.Left
        Me.Width = Me.Width - Diff
        lblHeading(4).Visible = False
        lblHeading(5).Visible = False
        lblHeading(9).Visible = False
        For i = 0 To 9
            txtCol5(i).Visible = False
            txtCol6(i).Visible = False
        Next
        lblPaste.Left = lblPaste.Left - 1150
      End If
      SetTooltips Mode
    End If
    
    For i = 1 To MaxDimen
       txtTime(i - 1) = Format(times(i), TIMEFORM)
       txtCol1(i - 1) = ZeroFormat(Values(1, i), FloatFormat)
       txtCol2(i - 1) = Values(2, i)
       txtCol3(i - 1) = ZeroFormat(Values(3, i), FloatFormat)
       txtCol4(i - 1) = Values(4, i)
       If Mode = 0 Then
          txtCol5(i - 1) = ZeroFormat(Values(5, i), FloatFormat)
          txtCol6(i - 1) = Values(6, i)
       End If
       txtCol7(i - 1) = ZeroFormat(Values(7, i), FloatFormat)
    Next i
    ' Belcourt added to always initialize
    txtTime(0) = SourceTerm.DelayTime
    DataModify = False
End Sub

